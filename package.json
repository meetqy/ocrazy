{
  "name": "ocrazy",
  "version": "0.0.4",
  "description": "🦆 `Tailwind CSS`+`React`实现的响应式 UI 组件",
  "homepage": "https://meetqy.github.io/ocrazy/",
  "author": "xiaoqiyang",
  "license": "MIT",
  "keywords": [
    "tailwind",
    "react",
    "mobile",
    "pc"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/meetqy/ocrazy"
  },
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build && npx tailwindcss -i ./src/main.css -o ./es/main.css",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "v": "npm version patch -m 'build: bump version to %s'",
    "prepublishOnly": "npm run build"
  },
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "files": [
    "es"
  ],
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "react": "^16.12.0 || ^17.0.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.4.0",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.3",
    "@umijs/fabric": "^2.8.1",
    "@umijs/test": "^3.0.5",
    "dumi": "^1.1.0",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "postcss-import": "^14.0.2",
    "prettier": "^2.2.1",
    "tailwindcss": "^3.0.23",
    "yorkie": "^2.0.0"
  }
}
